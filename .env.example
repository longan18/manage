# Docker environment settings
FRONTEND_TARGET=development  # development, build, or production
BACKEND_TARGET=development   # development, build, or production

# Node.js environment settings
NODE_ENV=development  # development or production

# Go environment settings
GO_ENV=development    # development or production
DEBUG=true            # true or false

# PostgreSQL settings
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=manage
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# Application settings
JWT_SECRET=your-secret-key
JWT_EXPIRATION=24h     # Token expiration time
API_PORT=8080          # Backend API port

# Frontend settings
NEXT_PUBLIC_API_URL=http://localhost:8080  # Backend API URL for frontend 